{
  "name": "@nju33/react-masonry",
  "description": "A masonry layout library where used React and Flexbox",
  "version": "1.0.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "repository": "nju33/react-masonry",
  "author": "nju33 <nju33.ki@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "masonry"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "yarn build",
    "dev": "webpack-dev-server",
    "build.demo": "webpack",
    "build.cjs": "tsc -P tsconfig.build.cjs.json",
    "build.esm": "tsc -P tsconfig.build.esm.json",
    "build.types": "tsc -P tsconfig.build.types.json --emitDeclarationOnly",
    "build": "yarn build.cjs && yarn build.esm && yarn build.types && yarn build.demo"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-components": "^3.4.2"
  },
  "devDependencies": {
    "@geekcojp/prettier-config": "0.3.0",
    "@geekcojp/tslint-config": "0.2.1",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/node": "10.12.11",
    "@types/randomcolor": "0.5.0",
    "@types/react": "16.7.10",
    "@types/react-dom": "16.0.11",
    "@types/webpack": "4.4.20",
    "html-webpack-plugin": "3.2.0",
    "prettier": "1.15.3",
    "randomcolor": "0.5.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "renovate-config-nju33": "0.0.1",
    "styled-components": "3.4.10",
    "ts-loader": "4.5.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {}
}
